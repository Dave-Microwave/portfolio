{% include 'incl/header.twig' %}

<body>
    {% include 'incl/nav.twig' %}

<div class="flex-row">
    <div class="tech-list flex">
        <h1>Tekniker</h1>
        {% for page in pages("technology") if not page.hidden %}
        <p class="tech-list-item" onclick="location.href='{{ page.url }}'"><a href="{{ page.url }}">{{ page.title }}</a></p>
        {% endfor %}
    </div>
    <div class="main technology" role="main">
        {{ content }}
    </div>
</div>
    {% include 'incl/footer.twig' %}
</body>

</html>